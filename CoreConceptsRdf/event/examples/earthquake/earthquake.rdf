<?xml version="1.0"?>

<!--
Abstract: RDF specification of an earthquake
Author: Marc Tim Thiemann
-->

<!-- Entities Declaration -->
<!DOCTYPE rdf:RDF [
    <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
]>

<!-- Namespaces Declaration-->
<rdf:RDF
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:xsd="&xsd;"
    xml:base="http://myearthquakes.com/">

<!-- Class for Event -->
<rdfs:Class rdf:about="Earthquake" />

</rdf:RDF>
